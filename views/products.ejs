              src="https://res.cloudinary.com/dn4nxz7f0/image/upload/f_auto,q_70,w_512/<%-post.file%>"
              alt="<%-post.title%>" />
            </div>
             
            
           
            <button type="button"
              class="<%-post.category%> button-card-category btn  mt-2 mb-2 pl-2 pr-2"><%=post.category%></button>
              <p class="text-date"><i class="fa fa-clock-o" aria-hidden="true"></i> <%=post.date%></p>
          </div>
          <div class="col-md-6 shadow p-4 card-blogpost-container">
              <h2 class="card-title-article"><%=post.title%></h2>
              <hr />
              <p class="card-text text-justify "><%-post.subtitle%></p>
              
           
     
            
              <p class="mt-4 pt-4">
                <a  title="<%-post.title%>" class="button-blog-post" href="/article/<%-post.slug%>">
                  Tiếp tục đọc
                 </a>
              </p>
               
              

            
          </div>
        </div>
      </div>
        <% }) %>
  
    </div>
  </div>

 
 
  
<div class="container">
<div class="row justify-content-center">
  <div class="col-sm-6">
  <% if (pages > 0) { %>
      <ul class="pagination text-center text-white">
          <% if (current == 1) { %>
              <li class="disabled page-item"><a class="page-link">Đầu</a></li>
          <% } else { %>
              <li class="page-item"><a class="page-link" href="/1">Đầu</a></li>
          <% } %>
          <% var i = (Number(current) > 5 ? Number(current) - 2 : 1) %>
          <% if (i !== 1) { %>
              <li class="disabled page-item"><a class="page-link">...</a></li>
          <% } %>
          <% for (; i <= (Number(current) + 2) && i <= pages; i++) { %>
              <% if (i == current) { %>
                  <li class=" page-item active"><a class="page-link"><%= i %></a></li>
              <% } else { %>
                  <li class="page-item"><a class="page-link" href="/<%= i %>"><%= i %></a></li>
              <% } %>
              <% if (i == Number(current) + 2 && i < pages) { %>
                  <li class="disabled page-item"><a class="page-link">...</a></li>
              <% } %>
          <% } %>
          <% if (current == pages) { %>
              <li class="disabled page-item"><a class="page-link">Cuối</a></li>
          <% } else { %>
              <li class="page-item" ><a href="/<%= pages %>" class="page-link">Cuối</a></li>
          <% } %>
      </ul>
  <% } %>
    </div>
  </div>
</div>

